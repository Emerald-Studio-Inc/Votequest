# Frontend Environment Variables

# Public RPC URLs (safe to expose in frontend)
NEXT_PUBLIC_POLYGON_AMOY_RPC_URL=https://rpc-amoy.polygon.technology
NEXT_PUBLIC_SEPOLIA_RPC_URL=https://rpc.sepolia.org

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# WalletConnect Project ID (get from https://cloud.walletconnect.com/)
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=your_walletconnect_project_id

# Contract Addresses (will be auto-updated by deployment script)
NEXT_PUBLIC_VOTEQUEST_ADDRESS_AMOY=0x77405d9D0a34D6eb59bC57dd5F434F8902CC5a5d
NEXT_PUBLIC_VOTEQUEST_ADDRESS_SEPOLIA=

# Default Chain ID (80002 for Polygon Amoy, 11155111 for Sepolia)
NEXT_PUBLIC_DEFAULT_CHAIN_ID=80002
